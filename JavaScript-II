<script>
function tabloCiz(){
var tabloIcerigi = "<table>";
for (var i=1; i<=10; i++){
tabloIcerigi +="<tr>";
 for (var j=1; j<=10; j++){
 tabloIcerigi +="<td width='25px' height='25px'>"+i*j+"</td>"; 
 }
 tabloIcerigi +="</tr>";
}
tabloIcerigi +="</table>";
document.write(tabloIcerigi);
}
document.onReady=tabloCiz();
</script>
