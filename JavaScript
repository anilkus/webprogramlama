<script>

function saat(){
var zaman = new Date();
document.write(zaman.getDate());
document.write(".");
document.write(zaman.getMonth()+1);
document.write(".");
document.write(zaman.getFullYear());
document.write("<br>");
}
saat();
//ikinci fonksiyonda saat dakika ve saniye yaz覺ls覺n
function saat2(){
var zaman=new Date();
document.write(zaman.getHours());
document.write(":");
document.write(zaman.getMinutes());
document.write(":");
document.write(zaman.getSeconds());
document.write("<br>");
}
saat2();
</script>
